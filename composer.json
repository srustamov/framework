  "license": "MIT",
  "authors": [
    {
      "name": "Samir Rustamov",
      "email": "rustemovv96@gmail.com",
      "homepage": "https://github.com/srustamov/framework",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "windwalker/edge": "~3.0",
    "phpmailer/phpmailer": "^6.0",
    "predis/predis": "^1.1",
    "filp/whoops": "^2.4",
    "lcobucci/jwt": "^3.3",
    "symfony/console": "^4.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "TT\\": "src/"
    },
    "files":[
      "src/Engine/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
